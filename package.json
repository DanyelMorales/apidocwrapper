  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.42",
    "@types/node": "^9.4.5",
    "chai": "^4.1.2",
    "grunt": "^1.0.2",
    "grunt-banner": "^0.6.0",
    "grunt-bump": "^0.8.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-exec": "^3.0.0",
    "mocha": "^3.5.0",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^3.1.1",
    "tspath": "^1.2.10",
    "typescript": "2.4.2"
  }
